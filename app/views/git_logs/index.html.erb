<h1>Listing git_logs</h1>

<table>
  <tr>
    <th>Authors</th>
    <th>Repository</th>
    <th>Comment</th>
    <th>Date</th>
  </tr>
   <script language="javascript" type="text/javascript" src="/javascripts/datetimepicker.js">
   </script>

  <div>   
    <input type="Text" id="demo1" maxlength="25" size="25">
    <a href="javascript:NewCal('demo1','ddmmmyyyy',true,24)">
    <img src="/images/cal.gif" width="16" height="16" border="0" alt="Pick a date"></a>
    <span class="descriptions">Start date</span>
  </div>

  <br>	

  <div>	
    <input type="Text" id="demo2" maxlength="25" size="25">
    <a href="javascript:NewCal('demo2','ddmmmyyyy',true,24)">
    <img src="/images/cal.gif" width="16" height="16" border="0" alt="Pick a date"></a>
    <span class="descriptions">End date</span>
  </div>


  <% @git_logs.each do |git_log| %>
    <tr>
      <td><%= link_to 'Show', git_log %></td>
      <td><%= link_to 'Edit', edit_git_log_path(git_log) %></td>
      <td><%= link_to 'Destroy', git_log, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New Git log', new_git_log_path %>
